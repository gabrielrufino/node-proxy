{
  "openapi": "3.0.3",
  "info": {
    "title": "Node Proxy API"
  },
  "servers": [],
  "components": {
    "securitySchemes": {
      "Token": {
        "type": "apiKey",
        "in": "query",
        "name": "token"
      }
    },
    "parameters": {
      "proxyId": {
        "in": "path",
        "name": "proxyId",
        "schema": {
          "type": "string"
        },
        "required": true
      }
    },
    "schemas": {
      "ProxyParams": {
        "type": "object",
        "required": [
          "namespace",
          "target"
        ],
        "properties": {
          "namespace": {
            "type": "string"
          },
          "target": {
            "type": "string"
          }
        }
      },
      "Proxy": {
        "allOf": [
          { "$ref": "#/components/schemas/ProxyParams" },
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "createdAt": {
                "type": "string"
              }
            }
          }
        ]
      }
    }
  },
  "paths": {
    "/health": {
      "get": {
        "tags": ["Health"],
        "responses": {
          "200": {
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/proxies": {
      "get": {
        "tags": ["Proxies"],
        "security": [{ "Token": [] }],
        "responses": {
          "200": {
            "content": {
              "application/json": {}
            }
          }
        }
      },
      "post": {
        "tags": ["Proxies"],
        "security": [{ "Token": [] }],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProxyParams"
              }
            }
          }
        },
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Proxy"
                }
              }
            }
          }
        }
      }
    },
    "/proxies/{proxyId}": {
      "get": {
        "tags": ["Proxies"],
        "security": [{ "Token": [] }],
        "parameters": [
          { "$ref": "#/components/parameters/proxyId" }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Proxy"
                }
              }
            }
          }
        }
      },
      "patch": {
        "tags": ["Proxies"],
        "security": [{ "Token": [] }],
        "parameters": [
          { "$ref": "#/components/parameters/proxyId" }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProxyParams"
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Proxy"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["Proxies"],
        "security": [{ "Token": [] }],
        "parameters": [
          { "$ref": "#/components/parameters/proxyId" }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Proxy"
                }
              }
            }
          }
        }
      }
    }
  }
}