{
  "name": "node-proxy",
  "version": "4.3.22",
  "private": true,
  "description": "Proxy made in Node.js",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "node -r dotenv/config src/index.js",
    "start:dev": "nodemon --exec 'npm start' --config nodemon.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "reinstall": "npx rimraf node_modules && npm ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielrufino/node-proxy.git"
  },
  "author": "Gabriel Rufino <contato@gabrielrufino.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielrufino/node-proxy/issues"
  },
  "homepage": "https://github.com/gabrielrufino/node-proxy#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "express-validator": "^6.15.0",
    "helmet": "^6.2.0",
    "lowdb": "^5.1.0",
    "pino": "^8.15.1",
    "pino-http": "^8.5.0",
    "prom-client": "^14.2.0",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@gabrielrufino/eslint-config": "^1.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.4",
    "jsdoc": "^4.0.2",
    "nodemon": "^2.0.22",
    "pino-pretty": "^9.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "funding": "https://github.com/sponsors/gabrielrufino"
}
