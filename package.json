{
  "name": "node-proxy",
  "version": "4.3.13",
  "private": true,
  "description": "Proxy made in Node.js",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev:setup": "cp .env.example .env",
    "dev:start": "dotenv nodemon src/index.js --config nodemon.json",
    "lint": "npx standard",
    "lint:fix": "npm run lint -- --fix",
    "start": "dotenv node src/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "reinstall": "npx rimraf node_modules && npm ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielrufino/node-proxy.git"
  },
  "author": "Gabriel Rufino <contato@gabrielrufino.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielrufino/node-proxy/issues"
  },
  "homepage": "https://github.com/gabrielrufino/node-proxy#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "express-validator": "^6.14.3",
    "helmet": "^6.0.1",
    "lowdb": "^5.1.0",
    "pino": "^8.8.0",
    "pino-http": "^8.3.2",
    "prom-client": "^14.1.1",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "jest": "^29.4.1",
    "jsdoc": "^4.0.0",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "rimraf": "^3.0.2",
    "standard": "^17.0.0",
    "supertest": "^6.3.3"
  },
  "funding": "https://github.com/sponsors/gabrielrufino"
}
